.PROGRAM m_cutting() #15950
;Cutting movements
  JMOVE #fi_spindle_c
;
;                   +----------------- start mandrino cappuccio
;                   |       +--------- comando start mandrino cappuccio
;                   |       |    +---- controllo mandrino in movimento
  CALL spindle_rot("START","ON","OFF")
;
  CALL mov_par(critical_speed,200,100,100)
;
  DO
;
;   +--search for the next enabled 'acn' (active cut number)
    DO
      IF rcp_cut_ena[acn]<>1 THEN
        acn = acn+1
      END
    UNTIL rcp_cut_ena[acn]==1 OR acn=>4
;
;.... and execute it
    IF rcp_cut_ena[acn]==1 THEN
;     +-- set speed mandrino
      CALL set_spin_speed
;     +-- set TOOL
      CALL set_tool("CUTTER")
;     +-- calcolo posizioni di taglio
      CALL calc_cut
;
      JMOVE #cut_p0
;                       +----------------- start mandrino cappuccio
;                       |       +--------- comando start mandrino cappuccio
;                       |       |     +--- controllo mandrino in movimento
      CALL spindle_rot("START","OFF","ON")
;
      JMOVE #cut_p1
      CALL mov_par(critical_speed/10,5,100,100)
      spin_status = 90+acn  ;91/92/93/94 single cut NOT completed
      ABS.SPEED ON
      SPEED rcp_cut_sp[acn] MM/S
      LMOVE #cut_p2
      BREAK
      IF rcp_cut_lift[acn]==0 THEN
        SPEED 100 MM/S
        LMOVE #cut_p3
        ABS.SPEED OFF
      ELSE
        SPEED 50 MM/S
        LMOVE #cut_p3
        BREAK
        ABS.SPEED OFF
      END
      CALL mov_par(critical_speed,200,100,100)
      LMOVE #cut_p4
      JMOVE #cut_p0
;
      spin_status = 10+acn  ;11/12/13/14 single cut completed
      last_cutter = cutter_status
;     +-- controllo fine vita coltelli
      cutter_life[cutter_status] = cutter_life[cutter_status]+1
      IF cutter_life[cutter_status]>max_cutt_life[cutter_status] THEN
        i_warning = cutter_status
      END
;
      CASE acn OF
       VALUE 1:
        IF rcp_mand_soff[1]>0 AND (rcp_cut_ena[2]==1 OR rcp_cut_ena[3]==1 OR rcp_cut_ena[4]==1) THEN
          IF rcp_mand_stop[1]==1 THEN
            CALL spindle_rot("STOP","ON","ON")
          END
          JMOVE #fi_spindle_c
          BREAK
          PULSE y_mand_soffio,rcp_mand_soff[1]
          TWAIT rcp_mand_soff[1]
          IF rcp_mand_stop[1]==1 THEN
            CALL spindle_rot("START","ON","ON")
          END
        END
       VALUE 2:
        IF rcp_mand_soff[2]>0 AND (rcp_cut_ena[3]==1 OR rcp_cut_ena[4]==1) THEN
          IF rcp_mand_stop[2]==1 THEN
            CALL spindle_rot("STOP","ON","ON")
          END
          JMOVE #fi_spindle_c
          BREAK
          PULSE y_mand_soffio,rcp_mand_soff[2]
          TWAIT rcp_mand_soff[2]
          IF rcp_mand_stop[2]==1 THEN
            CALL spindle_rot("START","ON","ON")
          END
        END
       VALUE 3:
        IF rcp_mand_soff[3]>0 AND rcp_cut_ena[4]==1 THEN
          IF rcp_mand_stop[3]==1 THEN
            CALL spindle_rot("STOP","ON","ON")
          END
          JMOVE #fi_spindle_c
          BREAK
          PULSE y_mand_soffio,rcp_mand_soff[3]
          TWAIT rcp_mand_soff[3]
          IF rcp_mand_stop[3]==1 THEN
            CALL spindle_rot("START","ON","ON")
          END
        END
      END
;
      JMOVE #cut_p0
      acn = acn+1
    END
;
  UNTIL acn=>4
;
;                   +--------------- stop mandrino cappuccio
;                   |      +-------- comando stop mandrino cappuccio
;                   |      |    +--- controllo mandrino fermo
  CALL spindle_rot("STOP","ON","OFF")
;
  CALL mov_par(critical_speed,200,100,100)
  JMOVE #fi_spindle_c
;
; +-- seleziona la prossima "prima" velocita' mandrino da utilizzare
  IF rcp_cut_ena[1]==1 THEN
    acn = 1
    CALL set_spin_speed
  ELSE
;
    IF rcp_cut_ena[2]==1 THEN
      acn = 2
      CALL set_spin_speed
    ELSE
;
      IF rcp_cut_ena[3]==1 THEN
        acn = 3
        CALL set_spin_speed
      ELSE
;
        IF rcp_cut_ena[4]==1 THEN
          acn = 4
          CALL set_spin_speed
        END
      END
    END
  END
;
  RETURN
.END
