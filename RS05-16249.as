.PROGRAM spindle_clamp(.$ope,.$break,.$comm,.$sens) #2142340
;Spindle clamp management
;
;   .$ope = "UNLOCK"  : release ring
;           "LOCK"    : lock ring
;
  IF .$break=="BREAK" THEN
    BREAK
  END
  IF rcp_spin_mode==0 THEN
;Close4lock
    SCASE .$ope OF
     SVALUE "LOCK":
      .$ope2 = "CLOSE"
     SVALUE "UNLOCK":
      .$ope2 = "OPEN"
    END
  ELSE
;Open4lock
    SCASE .$ope OF
     SVALUE "LOCK":
      .$ope2 = "OPEN"
     SVALUE "UNLOCK":
      .$ope2 = "CLOSE"
    END
  END
;
  IF NOT SWITCH(REPEAT ) THEN
    DELAY 0.5
    BREAK
    RETURN
  END
;
  SCASE .$ope2 OF
   SVALUE "OPEN":
    IF .$comm=="ON" THEN
;     +-- controllo pompa arburg accesa
      WHILE NOT SIG(x_arb_pompa) DO
        CALL alarm(21)
      END
      SIGNAL -y_close_spin
      PULSE y_open_spin,1
    END
    IF .$sens=="ON" THEN
      UTIMER .@time_out = 0
      WHILE NOT SIG(x_spin_open,-x_spin_closed) DO
        SIGNAL -y_close_spin
        PULSE y_open_spin
        IF UTIMER(.@time_out)>5 THEN
          CALL alarm(13)
          UTIMER .@time_out = 0
        END
        TWAIT 0.02
      END
    END
;
   SVALUE "CLOSE":
    IF .$comm=="ON" THEN
;     +-- controllo pompa arburg accesa
      WHILE NOT SIG(x_arb_pompa) DO
        CALL alarm(21)
      END
      SIGNAL -y_open_spin
      PULSE y_close_spin,1
    END
    IF .$sens=="ON" THEN
      UTIMER .@time_out = 0
      WHILE NOT SIG(-x_spin_open,x_spin_closed) DO
        SIGNAL -y_open_spin
        PULSE y_close_spin
        IF UTIMER(.@time_out)>5 THEN
          CALL alarm(14)
          UTIMER .@time_out = 0
        END
        TWAIT 0.02
      END
    END
  END
;
  RETURN
.END
.PROGRAM spindle_rot(.$ope,.$comm,.$sens) #3549488
;Spindle operation
;
  SCASE .$ope OF
   SVALUE "START":
;   +-- comando
    IF .$comm=="ON" THEN
      IF rcp_fase_mand==1 THEN
        PULSE y_spin_unlook,1
        UTIMER .@time_out = 0
        WHILE SIG(x_spin_unlook,-x_spin_look) DO
          IF UTIMER(.@time_out)>5 THEN
            CALL alarm(23)
            UTIMER .@time_out = 0
          END
          TWAIT 0.02
        END
        WAIT SIG(x_spin_unlook)
      ELSE
        IF SIG(-x_spin_unlook,x_spin_look) THEN
          PULSE y_spin_unlook,1
          UTIMER .@time_out = 0
          WHILE SIG(x_spin_unlook,-x_spin_look) DO
            IF UTIMER(.@time_out)>5 THEN
              CALL alarm(23)
              UTIMER .@time_out = 0
            END
            TWAIT 0.02
          END
          WAIT SIG(x_spin_unlook)
        END
      END
;	
      PULSE y_spin_start,1
    END
;   +-- attesa
    IF .$sens=="ON" THEN
      UTIMER .@time_out = 0
      WHILE NOT SIG(x_spin_rotating) DO
        IF UTIMER(.@time_out)>5 THEN
          CALL alarm(23)
          UTIMER .@time_out = 0
          PULSE y_spin_start,1
        END
        TWAIT 0.02
      END
    END
;
   SVALUE "STOP":
;   +-- comando
    IF .$comm=="ON" THEN
      PULSE y_spin_stop,0.3
    END
;   +-- attesa
    IF .$sens=="ON" THEN
      UTIMER .@time_out = 0
      WHILE NOT SIG(-x_spin_rotating) DO
        IF UTIMER(.@time_out)>5 THEN
          CALL alarm(24)
          UTIMER .@time_out = 0
          PULSE y_spin_stop,0.3
        END
        TWAIT 0.02
      END
    END
;
;   +-- controllo mandrino fermo e blocco orientato
    IF rcp_fase_mand==1 AND NOT SIG(x_spin_look) THEN
      UTIMER .@time_out = 0
      WAIT SIG(x_orient_spin,-x_spin_rotating)
      TWAIT 2 ; modifica qui!!
      TYPE 3: "twait"
      BREAK
      PULSE y_spin_look,1
;
      UTIMER .@time_out = 0
      WHILE SIG(x_spin_unlook,-x_spin_look) DO
        IF UTIMER(.@time_out)>10 THEN
          CALL alarm(23)
          UTIMER .@time_out = 0
        END
        TWAIT 0.02
      END
    END
;	
  END
;
  RETURN
.END
.PROGRAM spindle_verify(.spindle_empty) #2586647
