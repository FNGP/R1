   VALUE 1:
    CALL cutter_clamp("LOCK","BREAK")
    CALL cutter_holder(1,"UNLOCK","BREAK")
    CALL cutter_holder(2,"LOCK","BREAK")
   VALUE 2:
    CALL cutter_clamp("LOCK","BREAK")
    CALL cutter_holder(1,"LOCK","BREAK")
    CALL cutter_holder(2,"UNLOCK","BREAK")
  END
;
  RETURN
.END
