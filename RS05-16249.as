.PROGRAM optimize(.#in,.#out,.#ref) #14064906
;Optimize JT4 and JT6 cut position 
;
  DECOMPOSE in[1] = .#in
  DECOMPOSE out[1] = .#in
  DECOMPOSE ref[1] = .#ref
  DO
    .ope = 0
    IF out[4]<-90 THEN
      .ope = 1 ;increase JT4
    ELSE
      IF out[4]>90 THEN
        .ope = 2 ;decrease JT4
      END
    END
;
    CASE .ope OF
     VALUE 1:
      out[4] = out[4]+180
      out[5] = -out[5]
      out[6] = out[6]-180
     VALUE 2:
      out[4] = out[4]-180
      out[5] = -out[5]
      out[6] = out[6]+180
    END
  UNTIL .ope==0
  DO
    IF ref[6]-out[6]>180 THEN
      out[6] = out[6]+360
    ELSE
      IF ref[6]-out[6]<180 THEN
        out[6] = out[6]-360
      END
    END
  UNTIL ABS(ref[6]-out[6])<180
;
  POINT .#out = #PPOINT(out[1],out[2],out[3],out[4],out[5],out[6])
;
  RETURN
.END
