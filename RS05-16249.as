.PROGRAM calc_cut() #1281820
;Caculate cutting positions
;
  IF debug THEN
    TYPE 3: "CALC_CUT(",/F3.0,acn,/F0.2,"): diam=",rcp_cut_diam[acn],"  region=",rcp_region[acn],"  height=",rcp_cut_height[acn],"  thick=",rcp_cut_thick[acn]
    TYPE 3: /F0.2,"                ang1=",rcp_cut_ang1[acn],"  ang2=",rcp_cut_ang2[acn],"  lift=",rcp_cut_lift[acn],"  jig=",rcp_jig_height
    TYPE 3: /F0.2,"                blade_ang=",rcp_blade_ang,"  appro=",rcp_cut_appro[acn],"  RZ=",rcp_spin_rz[acn],"   BladeCorr=",rcp_blad_corr[acn]
    TYPE 3: /F0.2,"                blade_len=",rcp_blade_len
    TYPE 3: " "
  END
;
  IF rcp_region[acn]==0 THEN
;   +-- region 0: External
    POINT p1a = TRANS(rcp_cut_diam[acn]/2,0,rcp_jig_height+rcp_cut_height[acn])
  ELSE
;
;   +-- region 1: Internal
    POINT p1a = TRANS(-rcp_cut_diam[acn]/2,0,rcp_jig_height+rcp_cut_height[acn])
  END
  POINT p1 = p1a+RY(rcp_cut_ang1[acn]-rcp_blad_corr[acn])+org_cut+TRANS(,,-rcp_cut_appro[acn])+RZ(rcp_cut_ang2[acn]) ;start of cut
  POINT p0 = TRANS(0,0,45)+p1+TRANS(0,0,-30)
;
  cut_thick2 = rcp_cut_thick[acn]+rcp_cut_appro[acn]
  POINT p2 = p1+TRANS(0,cut_thick2*SIN(rcp_blad_corr[acn]),cut_thick2*COS(rcp_blad_corr[acn])) ;end of cut
;
  IF rcp_cut_lift[acn]>0 THEN
    POINT p3 = p2+TRANS(0,rcp_cut_lift[acn]*SIN(45),-rcp_cut_lift[acn]*COS(45))
  ELSE
;
    IF rcp_cut_lift[acn]<0 THEN
      POINT p3 = p2+TRANS(0,rcp_cut_lift[acn]*SIN(45),rcp_cut_lift[acn]*COS(45))
    ELSE
;
      POINT p3 = p1+TRANS(0,0,-10)
    END
  END
;
  IF rcp_region[acn]==0 THEN
;   +-- region 0: External
    POINT p4 = TRANS(0,0,30)+p3+TRANS(0,0,-rcp_cut_thick[acn]-10)
  ELSE
;
;   +-- region 1: Internal
    POINT p4 = TRANS(0,0,10)+p3+TRANS(0,0,-rcp_cut_thick[acn]-10)
  END
;
  POINT corr_fr_spin = TRANS(hmi_corr_spin_x,hmi_corr_spin_y,hmi_corr_spin_z)
;
  POINT #cut_p0t = fr_spin+corr_fr_spin+RZ(rcp_spin_rz[acn])+p0,#fi_spindle_c ;over
  POINT #cut_p1t = fr_spin+corr_fr_spin+RZ(rcp_spin_rz[acn])+p1,#fi_spindle_c ;start
  POINT #cut_p2t = fr_spin+corr_fr_spin+RZ(rcp_spin_rz[acn])+p2,#fi_spindle_c ;end
  POINT #cut_p3t = fr_spin+corr_fr_spin+RZ(rcp_spin_rz[acn])+p3,#fi_spindle_c ;lift
  POINT #cut_p4t = fr_spin+corr_fr_spin+RZ(rcp_spin_rz[acn])+p4,#fi_spindle_c ;retract
;
  CALL optimize(#cut_p0t,#cut_p0,#fi_spindle_c);1
  CALL optimize(#cut_p1t,#cut_p1,#fi_spindle_c);2
  CALL optimize(#cut_p2t,#cut_p2,#fi_spindle_c);3
  CALL optimize(#cut_p3t,#cut_p3,#fi_spindle_c);4
  CALL optimize(#cut_p4t,#cut_p4,#fi_spindle_c);5
;
  IF DEXT(#cut_p1,6)>DEXT(#cut_p0,6)+90 OR DEXT(#cut_p1,6)<DEXT(#cut_p0,6)-90 THEN
    CALL alarm(31)
    HALT
  END
  IF DEXT(#cut_p2,6)>DEXT(#cut_p0,6)+90 OR DEXT(#cut_p2,6)<DEXT(#cut_p0,6)-90 THEN
    CALL alarm(31)
    HALT
  END
  IF DEXT(#cut_p3,6)>DEXT(#cut_p0,6)+90 OR DEXT(#cut_p3,6)<DEXT(#cut_p0,6)-90 THEN
    CALL alarm(31)
    HALT
  END
  IF DEXT(#cut_p4,6)>DEXT(#cut_p0,6)+90 OR DEXT(#cut_p4,6)<DEXT(#cut_p0,6)-90 THEN
    CALL alarm(31)
    HALT
  END
;
  POINT zz_p1[acn] = p1
  POINT zz_p0[acn] = p1+TRANS(0,0,-15)
  POINT zz_p2[acn] = p2
  POINT zz_p3[acn] = p3
;
  RETURN
.END
.PROGRAM calc_pos_spin(.#appro,.#pick,.#place) #693292
;Calculate picking/placing position on the spindle
;
  .rot_clamp = 30 ;clamp rotation amount around Z
;
  ddh = rcp_ext_height
;
  TOOL t_clamp
;
; +-- search for the most distant appros position to spindle
  .mm = 1
  POINT .appro = NULL
  DO
    POINT .tmp = fr_spin+RZ(.rot_clamp)+TRANS(,,ddh+.mm)+org_clamp+RX(-2)
    IF INRANGE(.tmp,#fi_spindle_p)==0 THEN
      POINT .appro = .tmp
      .mm = .mm+1
    ELSE
      .mm = -9999 ;exit_loop   
    END
  UNTIL .mm==-9999
;
  POINT corr_fr_spin = TRANS(hmi_corr_spin_x,hmi_corr_spin_y,hmi_corr_spin_z)
;
  POINT .pick = fr_spin+corr_fr_spin+RZ(.rot_clamp)+TRANS(,,ddh)+org_clamp
  POINT .place = fr_spin+corr_fr_spin+RZ(.rot_clamp)+TRANS(,,ddh-hmi_jig_push)+org_clamp
;
  POINT .#appro = .appro,#fi_spindle_p
  POINT .#pick = .pick,#fi_spindle_p
  POINT .#place = .place,#fi_spindle_p
;
  RETURN
.END
.PROGRAM calc_pos_tab(.#rif,.#pos1,.#pos2,.#pos3,.#pos4,.#pos5) #6214759
;Calcolo posizioni presa/deposito piano inclinato
;
; +-- diametro anello capione
  .d_model = 106.8
; +-- altezza anello campione
  .h_model = 14.7
;
; +-- set tool pinza carico
  CALL set_tool("CLAMP")
; +-- riferimento
  POINT f_pincl+.rif = .#rif
; +-- avvicinamento
  POINT .#pos1 = f_pincl+SHIFT(.rif BY 0,(rcp_ext_diam-.d_model)/(2*SIN(60)),(rcp_ext_height-.h_model)+40),.#rif
; +-- presa anello
  POINT .#pos2 = f_pincl+SHIFT(.rif BY 0,(rcp_ext_diam-.d_model)/(2*SIN(60)),(rcp_ext_height-.h_model)),.#rif
; +-- posa anello
  POINT .#pos3 = f_pincl+SHIFT(.rif BY 0,(rcp_ext_diam-.d_model)/(2*SIN(60))+2,(rcp_ext_height-.h_model)+1),.#rif
; +-- allontanamento
  POINT .#pos4 = f_pincl+SHIFT(.rif BY 0,(rcp_ext_diam-.d_model)/(2*SIN(60))+10,(rcp_ext_height-.h_model)+10),.#rif
; +-- allontanamento
  POINT .#pos5 = f_pincl+SHIFT(.rif BY 0,(rcp_ext_diam-.d_model)/(2*SIN(60))+20,(rcp_ext_height-.h_model)+40),.#rif
;
  RETURN
.END
.PROGRAM cam_3d_risult() #9382
;Controllo risultati camera 3d
;
; +-- reset iniziale
  SIGNAL -m_cam3d_prep
  SIGNAL -m_cam3d_start
  IF SIG(y_cam_3d_trig) THEN
    b_cam_stop = TRUE
  END
  UTIMER @r_tim_result = 0
  UTIMER @r_tim_wait = 0
; +-- attesa pezzo buono/scarto o timeout
  WAIT SIG(x_cam_3d_buono) OR SIG(x_cam_3d_scarto) OR UTIMER(@r_tim_wait)>10 OR act_alarm==46
; +-- se sono in timeout
  IF UTIMER(@r_tim_wait)>10 THEN
    UTIMER @r_tim_wait = 0
;   +-- set pezzo scarto
    b_scarto_cam_3d = TRUE
;   +-- reset acquisizione a telecamera
    PULSE y_cam_3d_res,2
;   +-- segnalazione a op
    CALL alarm(47)
    RETURN
  END
;
; +-- Verifica risultato buono o scarto
  IF SIG(x_cam_3d_buono) THEN
    b_scarto_cam_3d = FALSE
  ELSE
    b_scarto_cam_3d = TRUE
  END
; +-- reset acquisizione a telecamera
  PULSE y_cam_3d_res,2
;	
.END
.PROGRAM camera_3d() #9595
; +-- Controllo telecamera 3d (sopra robot rs5)
;
; +-- inizializza variabili gestione telecamera
  SIGNAL -m_cam3d_prep
  SIGNAL -m_cam3d_start
  SIGNAL -m_cam3d_stop
;
; +-- set tool pinza
  CALL set_tool("CLAMP")
; +-- richiesta preparazione telecamera
  SIGNAL m_cam3d_prep
; +-- porta il robot in posizione (FUORI INGOMBRO DA TELCAMERA)
  i_muove_verso = 11
  CALL muovi_r
; +-- avvicinamento a telecamera
  JMOVE #avv_cam3d_01
; +-- posizione start controllo telecamera
  JMOVE #cam_3d_start
; +-- start acquisizione dati da telecamera
  BREAK
  SIGNAL m_cam3d_start
;
; +-- setto i valori di movimento (movimenti lenti)
  ABS.SPEED ON
  SPEED MINVAL(rcp_speed_lin,2),MINVAL(15,20) ALWAYS ; prove eseguite con velocita lin=0.7, circ=15
  ACCURACY 0.1 ALWAYS
  BREAK
  LMOVE #cam_3d_01a
  LMOVE #cam_3d_01b
  LMOVE #cam_3d_01
  BREAK
; +-- setto i valori di movimento (movimenti veloci)
  SPEED 10,35 ALWAYS
  LMOVE #cam_3d_02
  LMOVE #cam_3d_03
  LMOVE #cam_3d_04
  LMOVE #cam_3d_05
  BREAK
; +-- setto i valori di movimento (movimenti lenti)
  SPEED MINVAL(rcp_speed_circ,2),MINVAL(15,20) ALWAYS
; +-- posizione finale controllo telecamera
  LMOVE #cam_3d_stop
; +-- stop acquisizione dati da telecamera
  BREAK
  SIGNAL m_cam3d_stop
  BREAK
  ABS.SPEED OFF
;
; +-- set valori collision detection
  CALL mov_par(critical_speed,300,100,100)
; +-- allontanamento da telecamera
;LMOVE #all_cam3d_01
; +-- fuori ingombro da telecamera (finale)
  JMOVE #fi_end_cam
  i_muove_da = 12
;
  RETURN
.END
.PROGRAM chk_align(.#p1,.#p2) #5065354
;Check robot alignment in linear motion
;
  IF DEXT(.#p1,5)>0 AND DEXT(.#p2,5)<0 THEN
    b_align = ON
  END
  IF DEXT(.#p1,5)<0 AND DEXT(.#p2,5)>0 THEN
    b_align = ON
  END
  IF DEXT(.#p1,5)>-0.5 AND DEXT(.#p1,5)<0.5 THEN
    b_align = ON
  END
  IF DEXT(.#p2,5)>-0.5 AND DEXT(.#p2,5)<0.5 THEN
    b_align = ON
  END
  IF b_align==ON THEN
    CASE chk_acn OF
     VALUE 1:
      CALL alarm(35)
     VALUE 2:
      CALL alarm(36)
     VALUE 3:
      CALL alarm(37)
     VALUE 4:
      CALL alarm(38)
     ANY :
      HALT
    END
  END
;
  RETURN
.END
.PROGRAM chk_calc_cut() #780481
;Controllo posizioni di taglio
;
ini: b_align = OFF
  FOR chk_acn = 1 TO 4
    IF rcp_cut_ena[chk_acn]==1 THEN
;
      IF rcp_region[chk_acn]==0 THEN
;       +-- region 0: External
        POINT p1a = TRANS(rcp_cut_diam[chk_acn]/2,0,rcp_jig_height+rcp_cut_height[chk_acn])
      ELSE
;
;       +-- region 1: Internal
        POINT p1a = TRANS(-rcp_cut_diam[chk_acn]/2,0,rcp_jig_height+rcp_cut_height[chk_acn])
      END
      POINT p1 = p1a+RY(rcp_cut_ang1[chk_acn]-rcp_blad_corr[chk_acn])+org_cut+TRANS(,,-rcp_cut_appro[chk_acn])+RZ(rcp_cut_ang2[chk_acn]) ;start of cut
      POINT p0 = TRANS(0,0,30)+p1+TRANS(0,0,-30)
;
      cut_thick2 = rcp_cut_thick[chk_acn]+rcp_cut_appro[chk_acn]
      POINT p2 = p1+TRANS(0,cut_thick2*SIN(rcp_blad_corr[chk_acn]),cut_thick2*COS(rcp_blad_corr[chk_acn])) ;end of cut
;
      IF rcp_cut_lift[chk_acn]>0 THEN
        POINT p3 = p2+TRANS(0,rcp_cut_lift[chk_acn]*SIN(45),-rcp_cut_lift[chk_acn]*COS(45))
      ELSE
;
        IF rcp_cut_lift[chk_acn]<0 THEN
          POINT p3 = p2+TRANS(0,rcp_cut_lift[chk_acn]*SIN(45),rcp_cut_lift[chk_acn]*COS(45))
        ELSE
;
          POINT p3 = p1+TRANS(0,0,-10)
        END
      END
      IF rcp_region[chk_acn]==0 THEN
;       +-- region 0: External
        POINT p4 = TRANS(0,0,30)+p3+TRANS(0,0,-rcp_cut_thick[chk_acn]-10)
      ELSE
;
;       +-- region 1: Internal
        POINT p4 = TRANS(0,0,10)+p3+TRANS(0,0,-rcp_cut_thick[chk_acn]-10)
      END
;
      POINT corr_fr_spin = TRANS(hmi_corr_spin_x,hmi_corr_spin_y,hmi_corr_spin_z)
;
      POINT #cut_p0t = fr_spin+corr_fr_spin+RZ(rcp_spin_rz[chk_acn])+p0,#fi_spindle_c ;over
      POINT #cut_p1t = fr_spin+corr_fr_spin+RZ(rcp_spin_rz[chk_acn])+p1,#fi_spindle_c ;start
      POINT #cut_p2t = fr_spin+corr_fr_spin+RZ(rcp_spin_rz[chk_acn])+p2,#fi_spindle_c ;end
      POINT #cut_p3t = fr_spin+corr_fr_spin+RZ(rcp_spin_rz[chk_acn])+p3,#fi_spindle_c ;lift
      POINT #cut_p4t = fr_spin+corr_fr_spin+RZ(rcp_spin_rz[chk_acn])+p4,#fi_spindle_c ;retract
;
      CALL optimize(#cut_p0t,#cut_p0,#fi_spindle_c)
      CALL optimize(#cut_p1t,#cut_p1,#fi_spindle_c)
      CALL optimize(#cut_p2t,#cut_p2,#fi_spindle_c)
      CALL optimize(#cut_p3t,#cut_p3,#fi_spindle_c)
      CALL optimize(#cut_p4t,#cut_p4,#fi_spindle_c)
      CALL chk_align(#cut_p1,#cut_p2)
      CALL chk_align(#cut_p2,#cut_p3)
      CALL chk_align(#cut_p3,#cut_p4)
    END
  END
  IF b_align==ON GOTO ini
;
  RETURN
.END
.PROGRAM chk_cutt_clamp() #26371
