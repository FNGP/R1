.PROGRAM main() #11
;Main looping sequence called from 'Master() after 'Initialize()'
;
  CALL set_tool("CUTTER")
;
  DO
;   +-- wait conditions to perform one mission
    CALL wait_mission
;
;   +-- init punti
    CALL initpoints
;
;   +-- check the cutting movement
    CALL chk_calc_cut
;
;   +-- execute the selected mission
    CASE miss_exe OF
     VALUE 1:
;     +-- automatic direct transfer fro Robot1 to table
      CALL ring_transfer
;     +-- controllo richiesta fine ciclo
      IF SIG(x_end_cycle_req) THEN
;       +-- reset fine ciclo
        PULSE y_res_end_cycle,1
        UTIMER .@r_tim_wait = 0
        WHILE SIG(x_end_cycle_req) DO
          IF UTIMER(.@r_tim_wait)>5 THEN
            UTIMER .@r_tim_wait = 0
            PULSE y_res_end_cycle,1
          END
          TWAIT 0.02
        END
;       +-- porta il robot in posizione (HOME)
        i_muove_verso = 1
        CALL muovi_r
;       +-- reset ciclo automatico
        PULSE y_auto_cycle_ok,1
        UTIMER .@r_tim_wait = 0
        WHILE SIG(x_auto_enable) DO
          IF UTIMER(.@r_tim_wait)>5 THEN
            UTIMER .@r_tim_wait = 0
            PULSE y_auto_cycle_ok,1
          END
          TWAIT 0.02
        END
      END
;
     VALUE 2,11,12:
;     +-- cutting sequence for all rings
      CALL cut_sequence
;     +-- controllo richiesta fine ciclo
      IF SIG(x_end_cycle_req) THEN
;       +-- reset fine ciclo
        PULSE y_res_end_cycle,1
        UTIMER .@r_tim_wait = 0
        WHILE SIG(x_end_cycle_req) DO
          IF UTIMER(.@r_tim_wait)>5 THEN
            UTIMER .@r_tim_wait = 0
            PULSE y_res_end_cycle,1
          END
          TWAIT 0.02
        END
;       +-- porta il robot in posizione (HOME)
        i_muove_verso = 1
        CALL muovi_r
;       +-- reset ciclo automatico
        PULSE y_auto_cycle_ok,1
        UTIMER .@r_tim_wait = 0
        WHILE SIG(x_auto_enable) DO
          IF UTIMER(.@r_tim_wait)>5 THEN
            UTIMER .@r_tim_wait = 0
            PULSE y_auto_cycle_ok,1
          END
          TWAIT 0.02
        END
      END
;
     VALUE 5,16,17,24:
;     +-- controllo collisione con rs20
      CALL wait_spindle_sa
      SIGNAL -y_ficapp_rs20
;     +-- set ID taglio per anticipo presa coltello
      CALL set_id_taglio(.id_tag)
;     +-- presa coltello
      CALL cutter_change(rcp_cutter_n[.id_tag])
;     +-- porta il robot in posizione (HOME)
      i_muove_verso = 1
      CALL muovi_r
;     +-- reset controllo collisione con RS20
      SIGNAL y_ficapp_rs20
      IGNORE x_r1_safe_spind
;
     VALUE 4:
;     +-- porta il robot in posizione (HOME)
      i_muove_verso = 1
      CALL muovi_r
;     +-- controllo richiesta fine ciclo
      IF SIG(x_end_cycle_req) THEN
;       +-- reset fine ciclo
        PULSE y_res_end_cycle,1
        UTIMER .@r_tim_wait = 0
        WHILE SIG(x_end_cycle_req) DO
          IF UTIMER(.@r_tim_wait)>5 THEN
            UTIMER .@r_tim_wait = 0
            PULSE y_res_end_cycle,1
          END
          TWAIT 0.02
        END
      END
;
     VALUE 13:
;     +-- controllo collisione con rs20
      CALL wait_spindle_sa
      SIGNAL -y_ficapp_rs20
;     +-- deposito coltello
      CALL cutter_change(0)
;     +-- porta il robot in posizione (HOME)
      i_muove_verso = 1
      CALL muovi_r
;     +-- reset richiesta posa coltello
      CALL res_cutt_rel
;     +-- reset controllo collisione con RS20
      SIGNAL y_ficapp_rs20
      IGNORE x_r1_safe_spind
;
     VALUE 14:
;     +-- controllo collisione con rs20
      WAIT SIG(x_r1_safe_spind)
;     +-- porta il robot in posizione (MANDRINO)
      i_muove_verso = 5
      CALL muovi_r
;     +-- deposito coltello
      CALL cutter_change(0)
;     +-- posizione di cambiotipo
      CALL m_clamp_equip
;     +-- reset richiesta cambio tipo
      CALL res_clamp_eq
      HALT
;
     VALUE 15:
;     +-- move to home position
      CALL m_movehome
;
     VALUE 21:
;     +-- controllo collisione con rs20
      CALL wait_spindle_sa
      SIGNAL -y_ficapp_rs20
;     +-- porta il robot in posizione (MANDRINO)
      i_muove_verso = 5
      CALL muovi_r
;     +-- deposito anello negli sfridi
      CALL m_ring_reject
;     +-- porta il robot in posizione (HOME)
      i_muove_verso = 1
      CALL muovi_r
;     +-- reset controllo collisione con RS20
      SIGNAL y_ficapp_rs20
      IGNORE x_r1_safe_spind
;
     VALUE 22:
;     +-- controllo collisione con rs20
      CALL wait_spindle_sa
      SIGNAL -y_ficapp_rs20
;     +-- porta il robot in posizione (MANDRINO)
      i_muove_verso = 5
      CALL muovi_r
;     +-- presa anello da mandrino
      CALL m_pick_spindle
;     +-- deposito anello negli sfridi
      CALL m_ring_reject
;     +-- porta il robot in posizione (HOME)
      i_muove_verso = 1
      CALL muovi_r
;     +-- reset controllo collisione con RS20
      SIGNAL y_ficapp_rs20
      IGNORE x_r1_safe_spind
;
     VALUE 23:
;     +-- taglio singolo (manuale)
      CALL cut_single
;     +-- reset richiesta taglio singolo
      PULSE y_res_tag_sing,1
      UTIMER .@r_tim_wait = 0
      WHILE SIG(x_tag_sing) DO
        IF UTIMER(.@r_tim_wait)>5 THEN
          CALL alarm(41)
          PULSE y_res_tag_sing,1
          UTIMER .@r_tim_wait = 0
        END
        TWAIT 0.02
      END
;
     VALUE 25:
;     +-- controllo collisione con rs20
      WAIT SIG(x_r1_safe_spind)
;     +-- porta il robot in posizione (MANDRINO)
      i_muove_verso = 5
      CALL muovi_r
;     +-- controllo visivo coltello
      CALL m_cutter_check
;     +-- reset richiesta controllo visivo coltello
      CALL res_cutt_chk
      HALT
;
     VALUE 26:
;     +-- controllo RS20 in fuoriingombro
      CALL wait_r1_safe
;     +-- porta il robot in posizione (PIANO INCLINATO)
      i_muove_verso = 22
      CALL muovi_r
;     +-- preleva pezzo da tavola
      CALL m_pick_table
;     +-- controllo telecamera 3d
      CALL camera_3d
;     +-- controllo RS20 in fuoriingombro
      CALL wait_r1_safe
;     +-- porta il robot in posizione (PIANO INCLINATO)
      i_muove_verso = 22
      CALL muovi_r
      IGNORE x_r1_safe_spind
;     +-- deposito su piano inclinato
;                      +-- 2 = anello finito
      CALL m_place_table(2)
;     +-- muove in fuori ingombro
      JMOVE #fi_table
;     +-- reset ciclo rs5
      BREAK
      PULSE y_res_test,1
;
    END
;
  UNTIL FALSE
;
  RETURN
.END
