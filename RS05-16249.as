  LAPPRO #robot1_2[1],130
;
  i_muove_verso = 1
  CALL muovi_r
  HALT
;
  RETURN
.END
.PROGRAM aaa_set_up_rs5() #0
;--HOME POSITION                   STEP 13
;--TOOL CALCULATE                  STEP 18
;--AUTOTOOL GRIPPER                STEP 31
;--AUTOTOOL CUTTER                 STEP 52
;--FRAME TABLE                     STEP 89
;--RING TRANSFER TO RS20           STEP 100
;--PICK/PLACE CUTTER1 IN HOLDER    STEP 111
;--PICK/PLACE CUTTER2 IN HOLDER    STEP 129
;--FRAME SPINDLE                   STEP 145
;--DEPOSIT IN THE DRAWER           STEP 187
;--CHANGE TYPE                     STEP 197
;--CHECK CAMERA 3D                 STEP 204
;--Home position
  JMOVE #phome
;
  i_muove_verso = 5
  CALL muovi_r
;--TOOL CALCULATE
;-- base
  POINT new_base = RZ(135)+RY(-90)
  BASE new_base
;
  POINT t_clamp0 = atcr2+RX(-35)+RZ(180)
;POINT t_clamp = t_clamp0+TRANS(0,0,0)
;
;POINT t_cutter0 = TRANS(-22.5,-114.33,95.85)+RX(70) ;from drawings
  POINT t_cutter0 = atcr1+RX(70)+TRANS(,,-15)
  POINT t_cutter = t_cutter0+TRANS(,,rcp_blade_len)
  PAUSE
;
;--AUTOTOOL GRIPPER ( atcr2 )
;
  CALL set_tool("CLAMP")
  LMOVE #auto_1;              avvicinamento  Posture A1
  LMOVE #auto_1;              allontanamento Posture A1  
;
  LMOVE #auto_2;              avvicinamento  Posture A2
  LMOVE #auto_2;              allontanamento Posture A2 
;  
  LMOVE #auto_3;              avvicinamento  Posture A3
  LMOVE #auto_3;              allontanamento Posture A3 
;  
  LMOVE #auto_4;              avvicinamento  Posture A4
  PAUSE
  POINT t_clamp0 = atcr2+RX(-35)+RZ(180)
  POINT t_clamp = t_clamp0+TRANS(0,0,-39.4+17.5)
  TOOL t_clamp
  PAUSE
  LMOVE #auto_4;              allontanamento Posture A4
  PAUSE
;
;--AUTOTOOL CUTTER ( atcr1 )
;
  CALL set_tool("CUTTER")
  LMOVE #auto_10;             avvicinamento  Posture A1
  LMOVE #auto_10;             allontanamento Posture A1  
;
  LMOVE #auto_20;             avvicinamento  Posture A2
  LMOVE #auto_20;             allontanamento Posture A2 
;  
  LMOVE #auto_30;             avvicinamento  Posture A3
  LMOVE #auto_30;             allontanamento Posture A3 
;  
  LMOVE #auto_40;             avvicinamento  Posture A4
  PAUSE
  POINT t_cutter0 = atcr1+RX(70)+TRANS(,,-15)
  POINT t_cutter = t_cutter0+TRANS(,,rcp_blade_len)
  TOOL t_cutter
  PAUSE
  LMOVE #auto_40;             allontanamento Posture A4
  PAUSE
;
;--FRAME TABLE
;
  CALL set_tool("CUTTER")
  JAPPRO x1_pincl,50
  JMOVE x1_pincl; A+POS MOD
  LDEPART 50
  JAPPRO x2_pincl,50
  JMOVE x2_pincl; A+POS MOD
  LDEPART 50
  JAPPRO y1_pincl,50
  JMOVE y1_pincl; A+POS MOD
  LDEPART 50
;
  POINT f_pincl = FRAME(x1_pincl,x2_pincl,y1_pincl,x1_pincl)
  PAUSE
;
; +-- position table
  CALL set_tool("CLAMP")
  LAPPRO #rif_table1,30
  LMOVE #rif_table1;-h1- A+POS MOD
  LAPPRO #rif_table1,30
;
  LAPPRO #rif_table5,30
  LMOVE #rif_table5;-h1- A+POS MOD
  LAPPRO #rif_table5,30
  JMOVE #fi_table
;
; +-- ring transfer to rs20
  i_muove_verso = 25
  CALL muovi_r
;
  JAPPRO #robot1_2[1],130
  LMOVE #robot1_2[1]
  LAPPRO #robot1_2[1],130
; 
  i_muove_verso = 1
  CALL muovi_r
;  
; +-- pick and place cutter 1 in holder
  i_muove_verso = 5
  CALL muovi_r
  CALL set_tool("CUTTER")
  JMOVE #fi_cutters
  JMOVE #c_holder1001
  LMOVE #c_holder1002
  LMOVE #c_holder1001
  JMOVE #fi_cutters
;
;-- posa coltello 1
  CALL set_tool("CUTTER")
  JMOVE #fi_cutters
  JMOVE #c_holder1001
  LMOVE #c_holder1002
  LMOVE #c_holder1001
  JMOVE #fi_cutters
;
;-- PICK AND PLACE CUTTER 2 IN HOLDER
  CALL set_tool("CUTTER")
  JMOVE #fi_cutters
  JMOVE #c_holder2001
  LMOVE #c_holder2002
  LMOVE #c_holder2001
  JMOVE #fi_cutters
;
;-- posa coltello 2
  CALL set_tool("CUTTER")
  JMOVE #fi_cutters
  JMOVE #c_holder2001
  LMOVE #c_holder2002
  LMOVE #c_holder2001
  JMOVE #fi_cutters
;
;--FRAME SPINDLE
;
  i_muove_verso = 5
  CALL muovi_r
;
  CALL set_tool("CUTTER")
  JAPPRO #fsx1,20
  LMOVE #fsx1;(H.06 Xbase-)
  LAPPRO #fsx1,20
  LAPPRO #fsx2,20
  LMOVE #fsx2;(H.12 Xbase+)
  LAPPRO #fsx2,20
  LAPPRO #fsy1,20
  LMOVE #fsy1;(H.09 Ybase-)
  LAPPRO #fsy1,20
  LAPPRO #fsy2,20
  LMOVE #fsy2;(H.03 Ybase+)
  LAPPRO #fsy2,20
  PAUSE
;  
;Calculate spindle posture and frame
  TOOL t_cutter
  BASE new_base
  POINT fsx1 = #fsx1
  POINT fsx2 = #fsx2
  POINT fsy1 = #fsy1
  POINT fsy2 = #fsy2
;
  ddx = DEXT(fsx2,1)-DEXT(fsx1,1)
  ddz1 = DEXT(fsx2,3)-DEXT(fsx1,3)
  rot_y = ATAN2(ddz1,ddx)
;
  ddy = DEXT(fsy2,2)-DEXT(fsy1,2)
  ddz2 = DEXT(fsy2,3)-DEXT(fsy1,3)
  rot_x = ATAN2(ddz2,ddy)
;
  POINT spin_posture = RX(180)+RX(rot_x)+RY(rot_y)
  POINT fr_spin = CCENTER(fsy1,fsx2,fsy2,spin_posture)
  POINT org_cut = RZ(90)+RY(180)
  POINT org_clamp = RZ(90)+RY(180)
  PAUSE
;
;--DEPOSIT IN THE DRAWER
;
  CALL set_tool("CLAMP")
  JMOVE #fi_spindle_p
  JMOVE #reject1
  LMOVE #reject2
  LMOVE #reject1
  JMOVE #fi_spindle_p
;
;-- CHANGE TYPE
  CALL set_tool("CLAMP")
  JMOVE #fi_cutters
  JMOVE #clamp_eq1
  PAUSE
  JMOVE #clamp_eq1
  JMOVE #fi_cutters
;
;-- CHECK CAMERA 3D
;
; +--position for camera frame
  CALL set_tool("CUTTER")
  JAPPRO x1_cam3d,50
  JMOVE x1_cam3d; A+POS MOD
  LDEPART 50
  JAPPRO x2_cam3d,50
  JMOVE x2_cam3d; A+POS MOD
  LDEPART 50
  JAPPRO y1_cam3d,50
  JMOVE y1_cam3d; A+POS MOD
  LDEPART 50
;
  POINT f_cam = FRAME(x1_cam3d,x2_cam3d,y1_cam3d,x1_cam3d)
  PAUSE
;
;
; +-- origin for position
  JAPPRO #org_cam3d1,50
  JMOVE #org_cam3d1
  JAPPRO #org_cam3d1,50
  PAUSE
;
; +-- position calculation
  CALL set_tool("CLAMP")
;
; +-- N.B. ogni posizione e calcolata rispetto alla precedente
;     quindi tutti gli offset aggiunti ad ogni posizione non sono riferiti all'origine
;     ma alla posizione precedente. Nel commento e inserito l'offset rispetto all'origine
;
; +-- posizione START spostata rispetto all'origine
  POINT f_cam+org_cam3d1 = #org_cam3d1
  POINT #cam_3d_start = f_cam+SHIFT(org_cam3d1 BY -15,-95,25)+TRANS(0,0,0,0,0,90),#org_cam3d1
; +-- posizione 1a per rallentamento su "buchi"
  POINT .#origine = #cam_3d_start
  POINT f_cam+.attuale = .#origine
  POINT #cam_3d_01a = f_cam+SHIFT(.attuale BY 0,55,0)+TRANS(0,0,0,0,0,0),.#origine
; +-- posizione 1b per rallentamento su "buchi"
  POINT .#origine = #cam_3d_start
  POINT f_cam+.attuale = .#origine
  POINT #cam_3d_01b = f_cam+SHIFT(.attuale BY 0,155,0)+TRANS(0,0,0,0,0,0),.#origine
; +-- posizione 1 spostata rispetto all'origine
  POINT .#origine = #cam_3d_start
  POINT f_cam+.attuale = .#origine
  POINT #cam_3d_01 = f_cam+SHIFT(.attuale BY 0,200,0)+TRANS(0,0,0,0,0,0),.#origine
; +-- posizione 2 spostata rispetto all'origine
  POINT .#origine = #cam_3d_01
  POINT f_cam+.attuale = .#origine
  POINT #cam_3d_02 = f_cam+SHIFT(.attuale BY 55,-40,0)+TRANS(0,0,0,0,0,45),.#origine
; +-- posizione 3 spostata rispetto all'origine
  POINT .#origine = #cam_3d_02
  POINT f_cam+.attuale = .#origine
  POINT #cam_3d_03 = f_cam+SHIFT(.attuale BY 20,-70,0)+TRANS(0,0,0,0,0,50),.#origine
; +-- posizione 4 spostata rispetto all'origine
  POINT .#origine = #cam_3d_03
  POINT f_cam+.attuale = .#origine
  POINT #cam_3d_04 = f_cam+SHIFT(.attuale BY -40,-80,0)+TRANS(0,0,0,0,0,50),.#origine
; +-- posizione 5 spostata rispetto all'origine
  POINT .#origine = #cam_3d_04
  POINT f_cam+.attuale = .#origine
  POINT #cam_3d_05 = f_cam+SHIFT(.attuale BY -50,0,0)+TRANS(0,0,0,0,0,35),.#origine
; +-- posizione 6 spostata rispetto all'origine
  POINT .#origine = #cam_3d_05
  POINT f_cam+.attuale = .#origine
  POINT #cam_3d_stop = f_cam+SHIFT(.attuale BY 0,180,0)+TRANS(0,0,0,0,0,0),.#origine
  PAUSE
;
; +-- test movimenti camera 3d
  i_muove_da = 1
  i_muove_verso = 12
  CALL muovi_r
  JMOVE #fi_start_cam
  LMOVE #avv_cam3d_01
  PAUSE
; +-- scansione camera 3d
  LMOVE #cam_3d_start
  LMOVE #cam_3d_01a
  LMOVE #cam_3d_01b
  LMOVE #cam_3d_01
  LMOVE #cam_3d_02
  LMOVE #cam_3d_03
  LMOVE #cam_3d_04
  LMOVE #cam_3d_05
  LMOVE #cam_3d_stop
  PAUSE
; +-- allontanamento
;  LMOVE #all_cam3d_01
  JMOVE #fi_end_cam
  PAUSE
  i_muove_da = 12
  i_muove_verso = 1
  CALL muovi_r
  HALT
;
  RETURN
.END
