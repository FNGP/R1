.PROGRAM x_move(.$target,.x_sens,.sp,.err) #716877
;Simulate an XMOVE command using LMOVE+BRAKE
;
;Preceeding error state
  IF .err==TRUE THEN
    IF debug THEN
      TYPE 3: "X_MOVE: error param already active"
    END
    RETURN
  END
;
;Signal is already active ---> error
  IF SIG(.x_sens) THEN
    .err = TRUE
    IF debug THEN
      TYPE 3: "X_MOVE: signal already active"
    END
    RETURN
  END
;
;Joint or location
  $pref = $LEFT(.$target,1)
  IF $pref=="#" THEN
    POINT .#target = STRTOPOS(.$target)
    POINT .target = .#target
  ELSE
    POINT .target = STRTOPOS(.$target)
    POINT .#target = .target
  END
;
;Motion
;ABS.SPEED ON
  DO
    ACCURACY 1
    SPEED .sp MM/S
    LMOVE .#target
    DO
      IF SIG(.x_sens) THEN
        .err = TRUE
        BRAKE
        ABS.SPEED OFF
        RETURN
      END
    UNTIL DISTANCE(.target,HERE)<1
  UNTIL DISTANCE(.target,HERE)<1
  ABS.SPEED OFF
;
  RETURN
.END
