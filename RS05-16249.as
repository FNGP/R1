.PROGRAM m_place_spindle() #713820
;Place ring on spindle
;
  CALL mov_par(critical_speed,300,100,100)
  JMOVE #fi_spindle_p
  CALL chk_spindle("PLACE")
retry:
  ACCURACY 10
  JMOVE #spin_appro
  .collision = FALSE
  CALL x_move("#spin_place",-x_r_clamp_coll,250,.collision)
  IF .collision==TRUE THEN
    SPEED 5
    LMOVE #spin_appro
    CALL alarm(15)
    GOTO retry
  ELSE
    CALL ring_clamp("UNLOCK","BREAK")
    CALL spindle_clamp("LOCK","","ON","")
    spin_status = 1
    clamp_status = 0
    acn = 1
    LMOVE #spin_appro
  END
  ACCURACY 10
  JMOVE #fi_spindle_p
  IF NOT .collision THEN
    CALL spindle_clamp("LOCK","","","ON")
  END
;
  RETURN
.END
.PROGRAM m_place_table(.table_status) #707220
;Place the completed ring on the table
;
  CALL mov_par(critical_speed,300,100,100)
  CALL wait_r1_safe
;
; +-- control position free
  CALL chk_pos_free(5)
;
  JMOVE #table4[5]
  LMOVE #table3[5]
  LMOVE #table2place[5]
  IF SIG(x_cam_3d_inc) THEN
;   +-- control cam 3d result
    CALL cam_3d_risult
  END
  CALL ring_clamp("UNLOCK","BREAK")
  clamp_status = 0
  STABLE 0.2
  tab_part_status[5] = .table_status
  LMOVE #table1[5]
  tab_part_status[5] = .table_status
;
; +-- verify ring deposit on table
  UTIMER .@r_tim_wait = 0
  WHILE NOT SIG(x_table_pres[5]) DO
    IF UTIMER(.@r_tim_wait)>3 THEN
      i_muove_verso = 1
      CALL muovi_r
      CALL alarm(33)
      UTIMER .@r_tim_wait = 0
    END
    TWAIT 0.02
  END
  UTIMER .@r_tim_wait = 0
;
  RETURN
.END
