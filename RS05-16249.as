  JMOVE #pos_zero
;	
.END
.PROGRAM wait_mission() #854578
;Attesa operazioni da eseguire
;
  miss_last = miss_exe
  miss_exe = 0
  BREAK
  DO
;   +-- verificare coerenza variabile 'cutter_status'
    CALL cutter_verify
;
;
;   +-- manuale
    IF SIG(x_man_enable) THEN
;
;
;     +-- posa coltello (manuale)
      IF SIG(x_cutt_rel_req,x_r1_safe_table,y_home1) AND cutter_status>0 THEN
        miss_exe = 13
        RETURN
      END
;
;
;     +-- cambio tipo (manuale)
      IF SIG(x_chng_type_req,x_r1_safe_table,y_home1) THEN
        miss_exe = 14
        RETURN
      END
;
;
;     +-- controllo visivo coltello (manuale)
      IF SIG(x_cutt_chk_req,x_r1_safe_table,y_home1) AND cutter_status>0 THEN
        miss_exe = 25
        RETURN
      END
;
;
;     +-- movements to home position
      IF SIG(x_movehome_req) THEN
        miss_exe = 15
        RETURN
      END
;
    END
;
;
;   +-- automatico
    IF SIG(x_auto_enable) THEN
;
;     +-- ciclo automatico
      IF SIG(x_start_seq_req) AND SIG(x_r1_safe_table) AND SIG(x_r1_safe_spind) THEN
        IF rcp_cut_ena[1]==1 OR rcp_cut_ena[2]==1 OR rcp_cut_ena[3]==1 OR rcp_cut_ena[4]==1 THEN
          IF cutter_status<>0 AND SIG(m_raw_pres_p) THEN
            miss_exe = 2 ;taglio
            RETURN
          END
          IF cutter_status==0 THEN
            miss_exe = 5 ;cambio coltello
            RETURN
          END
        END
      END
    END
;
;   +-- operazioni manuali
    IF SIG(x_man_enable,x_r1_safe_table) THEN
      IF rcp_cut_ena[1]==1 OR rcp_cut_ena[2]==1 OR rcp_cut_ena[3]==1 OR rcp_cut_ena[4]==1 THEN
;
;       +-- ciclo solo taglio
        IF SIG(x_start_seq_req) THEN
          IF cutter_status<>0 AND SIG(m_raw_pres_p) THEN
            miss_exe = 11 ;taglio
            RETURN
          END
          IF cutter_status==0 THEN
            miss_exe = 16 ;cambio coltello
            RETURN
          END
        END
;
;       +-- richiesta taglio singolo
        IF SIG(x_tag_sing,y_home1) THEN
;         +-- se ho il coltello a bordo ed ho pezzi sul piano inclinato
          IF cutter_status<>0 AND SIG(m_raw_pres_p) THEN
;           +-- eseguo la sequenza di taglio
            miss_exe = 23
            RETURN
          END
;         +-- se non ho il coltello a bordo
          IF cutter_status==0 THEN
;           +-- presa coltello
            miss_exe = 24
            RETURN
          END
;         +-- se non ho pezzi sul piano inclinato
          IF NOT SIG(m_raw_pres_p) THEN
;           +-- reset richiesta taglio singolo
            PULSE y_res_tag_sing,1
;           +-- attesa reset taglio singolo
            UTIMER .@r_tim_wait = 0
            WHILE SIG(x_tag_sing) DO
              IF UTIMER(.@r_tim_wait)>5 THEN
                CALL alarm(41)
              END
              TWAIT 0.02
            END
          END
        END
      END
;
;     +-- ciclo test telecamera 3d
      IF SIG(x_test_cam,y_home1) THEN
;	    +-- se ho pezzi sul piano inclinato
        IF SIG(m_raw_pres_p) THEN
;           +-- eseguo la sequenza di controllo telecamera
          miss_exe = 26
          RETURN
        ELSE
;           +-- reset richiesta controllo telecamera
          PULSE y_res_test,3
;           +-- attesa reset taglio singolo
          UTIMER .@r_tim_wait = 0
          WHILE SIG(x_test_cam) DO
            IF UTIMER(.@r_tim_wait)>5 THEN
              CALL alarm(48)
            END
            TWAIT 0.02
          END
        END
      END
    END
;
;   +-- muove in home		
    IF SIG(x_end_cycle_req) AND NOT SIG(y_home1) THEN
      miss_exe = 4 ;muovihome
      RETURN
    END
;
;   +-- transfer ring from Robot1 to table
    IF SIG(m_tab_empty) THEN
      IF SIG(x_r1_direct) AND miss_last<>1 THEN
        miss_exe = 1
        RETURN
      END
    END
;
    TWAIT 0.05
  UNTIL miss_exe>0
;
  RETURN
.END
.PROGRAM wait_r1_safe() #1677749
;Wait for Robot1 in safe area from Table position
;
  UTIMER .@r_tim_wait = 0
